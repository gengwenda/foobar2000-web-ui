{
    "name": "foobar2000-web-ui",
    "version": "0.1.0",
    "author": "Matias Klemola <matias.klemola@gmail.com>",
    "description": "Control foobar2000 from the web.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/klemola/foobar2000-web-ui.git"
    },
    "keywords": [
        "foobar2000",
        "remote control",
        "server"
    ],
    "scripts": {
        "test-server": "mocha src/test/server",
        "start": "node src/Main.js",
        "test": "yarn test-server",
        "lint": "eslint src/*.js src/static/js/*.js",
        "format": "prettier --write src/**/*.js ./Config.js",
        "logs": "bunyan foobar2000-web-ui.log"
    },
    "dependencies": {
        "body-parser": "1.19.0",
        "bunyan": "1.8.12",
        "dtrace-provider": "0.8.7",
        "express": "4.16.4",
        "jade": "1.11.0",
        "lodash": "4.17.11",
        "minimist": "1.2.0",
        "socket.io": "2.2.0",
        "socket.io-client": "2.2.0"
    },
    "devDependencies": {
        "chai": "4.2.0",
        "eslint": "5.16.0",
        "eslint-config-prettier": "4.2.0",
        "eslint-plugin-prettier": "^3.0.1",
        "mocha": "6.1.4",
        "prettier": "^1.17.0"
    },
    "prettier": {
        "semi": false,
        "singleQuote": true
    },
    "engines": {
        "node": ">=12.1.0"
    }
}
