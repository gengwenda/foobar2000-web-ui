{
    "compilerOptions": {
        "moduleResolution": "node",
        "lib": ["dom", "dom.iterable", "esnext", "es2015.iterable"],
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "esModuleInterop": true,
        "strict": true,
        "noImplicitReturns": true,
        "noEmitOnError": true,
        "incremental": true
    }
}
