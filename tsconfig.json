{
    "compilerOptions": {
        "moduleResolution": "node",
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "allowJs": false,
        "esModuleInterop": true,
        "strict": true,
        "noImplicitReturns": true,
        "noEmitOnError": true,
        "incremental": true
    }
}
